<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Magasin" table="Magasin">
    <unique-constraints>
      <unique-constraint name="nomMagasin" columns="nomMagasin"/>
    </unique-constraints>
    <id name="idmagasin" type="integer" column="idMagasin">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nommagasin" type="string" column="nomMagasin" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adresse" type="string" column="adresse" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="idsalle" target-entity="Salle" inversed-by="idmagasin" fetch="LAZY">
      <join-table name="choisir">
        <join-columns>
          <join-column name="idMagasin" referenced-column-name="idMagasin"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="idSalle" referenced-column-name="idSalle"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
