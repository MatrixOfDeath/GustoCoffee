<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\FermetureDuCafe" table="Fermeture_du_Cafe">
    <indexes>
      <index name="dateDebut" columns="dateDebut,dateFin"/>
    </indexes>
    <id name="idfermeture" type="integer" column="idFermeture">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="datedebut" type="date" column="dateDebut" nullable="true"/>
    <field name="datefin" type="date" column="dateFin" nullable="true"/>
    <field name="titre" type="string" column="titre" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="raison" type="text" column="raison" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="jourferie" type="string" column="jourFerie" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
