<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Personne" table="Personne">
    <indexes>
      <index name="rulePersonne" columns="rulePersonne"/>
      <index name="FK_Personne_idRule" columns="idRule"/>
    </indexes>
    <id name="idpersonne" type="integer" column="idPersonne">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="indentifiant" type="string" column="indentifiant" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="motdepasse" type="string" column="motDePasse" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nom" type="string" column="nom" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenom" type="string" column="prenom" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adresse" type="string" column="adresse" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="codepostal" type="string" column="codePostal" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="statutpersonne" type="string" column="statutPersonne" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pointscumules" type="integer" column="pointsCumules" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="rulepersonne" type="integer" column="rulePersonne" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="newsletter" type="boolean" column="newsletter" nullable="true"/>
    <many-to-one field="idrule" target-entity="RuleUtilisateur" fetch="LAZY">
      <join-columns>
        <join-column name="idRule" referenced-column-name="idRule"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
