<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ModeDePaiement" table="Mode_de_Paiement">
    <indexes>
      <index name="modePaiement" columns="modePaiement"/>
    </indexes>
    <id name="idmodepaiement" type="integer" column="idModePaiement">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="modepaiement" type="string" column="modePaiement" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="idfacture" target-entity="Facture" mapped-by="idmodepaiement" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
