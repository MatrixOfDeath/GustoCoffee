<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\DemandeProduit" table="Demande_Produit">
    <indexes>
      <index name="FK_Demande_Produit_idPersonne" columns="idPersonne"/>
    </indexes>
    <id name="iddemandeproduit" type="integer" column="idDemandeProduit">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="datedemande" type="date" column="dateDemande" nullable="true"/>
    <field name="remarque" type="text" column="remarque" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idpersonne" target-entity="Personne" fetch="LAZY">
      <join-columns>
        <join-column name="idPersonne" referenced-column-name="idPersonne"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
